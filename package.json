{
  "name": "dante",
  "version": "1.0.0",
  "private": true,
  "description": "dante",
  "author": "Oskar Dudycz",
  "keywords": [
    "gatsby"
  ],
  "scripts": {
    "develop": "run-p develop:*",
    "develop:web": "gatsby develop",
    "develop:functions": "netlify dev",
    "start": "gatsby develop",
    "build": "run-s build:typecheck build:web",
    "build:typecheck": "tsc --noEmit",
    "build:web": "gatsby build",
    "serve": "run-p serve:*",
    "serve:web": "gatsby serve",
    "serve:functions": "netlify functions:serve",
    "clean": "gatsby clean",
    "lint": "run-s lint:eslint lint:prettier",
    "lint:eslint": "eslint **/*.{ts,tsx}",
    "lint:prettier": "prettier --check \"**/**/!(*.d).{ts,tsx,json,md,mdx}\"",
    "fix": "run-s fix:eslint fix:prettier",
    "fix:eslint": "eslint **/*.{ts,tsx} --fix",
    "fix:prettier": "prettier --write \"**/**/!(*.d).{ts,tsx,json,md,mdx}\"",
    "deploy": "netlify deploy",
    "deploy:prod": "netlify deploy --prod"
  },
  "dependencies": {
    "@graphql-eslint/eslint-plugin": "^3.10.7",
    "@mdx-js/mdx": "^2.1.2",
    "@mdx-js/react": "^2.1.2",
    "@netlify/functions": "^1.1.0",
    "@stripe/stripe-js": "^1.35.0",
    "axios": "^0.27.2",
    "axios-hooks": "^3.1.4",
    "gatsby": "^4.0.0-next.1",
    "gatsby-plugin-image": "^2.20.0",
    "gatsby-plugin-manifest": "^4.20.0",
    "gatsby-plugin-mdx": "^4.1.0-next.0",
    "gatsby-plugin-sharp": "^4.20.0",
    "gatsby-plugin-sitemap": "^5.20.0",
    "gatsby-source-filesystem": "^4.20.0",
    "gatsby-transformer-remark": "^5.20.0",
    "gatsby-transformer-sharp": "^4.20.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "stripe": "^10.2.0"
  },
  "devDependencies": {
    "@types/node": "^18.7.2",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.33.0",
    "@typescript-eslint/parser": "5.33.0",
    "autoprefixer": "^10.4.8",
    "dotenv": "^16.0.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-mdx": "^2.0.2",
    "eslint-plugin-prettier": "^4.2.1",
    "gatsby-plugin-postcss": "^5.20.0",
    "netlify-cli": "^10.17.2",
    "netlify-plugin-inline-functions-env": "^1.0.9",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.16",
    "tailwindcss": "^3.1.8",
    "typescript": "^4.7.4"
  }
}
